enable


erase start






configure terminal

hostname SW2_0


enable secret 123
username cisco privilege 15 secret 123

line console 0
login local
logging synchronous
exit

line vty 0 4
login local
logging synchronous
exit




vlan 10
name Estudiantes
exit

vlan 20
name Docentes
exit

vlan 99
name Manager
exit

vlan 100
name Agujero_negro
exit

interface vlan 99
ip address 192.168.91.3 255.255.255.0
Description Regula el flujo de la vlan 99
no shutdown
exit



interface F0/1
switchport mode access
switchport access vlan 10
no shutdown
exit


interface F0/2
switchport mode access
switchport access vlan 20
no shutdown
exit


interface G0/1
switchport mode trunk
switchport trunk native vlan 99
switchport trunk allowed vlan 10,20,99
Description Conecta con el SW3_0 para el flujo de las vlans mode trunk
no shutdown
exit


ip default-gateway 192.168.91.1


interface range F0/3-24, G0/2
switchport mode access
switchport access vlan 100
Description Puerto inactivo
shutdown
exit

no vlan 100 

end

